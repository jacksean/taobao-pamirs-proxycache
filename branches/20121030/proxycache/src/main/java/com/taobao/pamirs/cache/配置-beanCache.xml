<?xml version="1.0" encoding="GBK"?>
<cacheConfig>
	<!-- 缓存Bean配置 -->
	<cacheBeans list="true">
		<cacheBean>
			<beanName></beanName>
			<cacheMethods list="true">
				<methodConfig>
					<methodName></methodName>
					<parameterTypes list="true"><!-- 可选，如果有重载方法时，必须要指定 -->
						<String>{String}</String>
						<String>{String}</String>
					</parameterTypes>
					<expiredTime></expiredTime><!-- 可选，失效时间，单位：秒。可以是相对时间，也可以是绝对时间(大于当前时间戳是绝对时间过期)。不传或0都是不过期 -->
				</methodConfig>
				<methodConfig>
					<methodName></methodName>
					<parameterTypes list="true"><!-- 可选，如果有重载方法时，必须要指定 -->
						<String>{String}</String>
						<String>{String}</String>
					</parameterTypes>
					<expiredTime></expiredTime><!-- 可选，失效时间，单位：秒。可以是相对时间，也可以是绝对时间(大于当前时间戳是绝对时间过期)。不传或0都是不过期 -->
				</methodConfig>
			</cacheMethods>
		</cacheBean>
		<cacheBean>
			...
		</cacheBean>
	</cacheBeans>

	<!-- 缓存清理Bean配置 -->
	<cacheCleanBeans list="true">
		<cacheCleanBean>
			<beanName></beanName>
			<methods list="true">
				<cacheCleanMethod>
					<methodName></methodName>
					<parameterTypes list="true"><!-- 可选，如果有重载方法时，必须要指定 -->
						<String>String</String>
						<String>String</String>
					</parameterTypes>
					<cleanMethods>
						<methodConfig>
							<methodName>getActivityByCode</methodName>
							<beanName /><!-- 可选,默认继承上面的beanName -->
							<parameterTypes /><!-- 不用配置，参数必须和上面的一致 -->
						</methodConfig>
						<methodConfig>
							...
						</methodConfig>
					</cleanMethods>
				</cacheCleanMethod>
				<cleanMethod>
					...
				</cleanMethod>
			</methods>
		</cacheCleanBean>
		<cacheCleanBean>
			...
		</cacheCleanBean>
	</cacheCleanBeans>

</cacheConfig>
