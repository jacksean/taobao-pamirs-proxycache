<?xml version="1.0" encoding="GBK"?>
<cacheModule>
  <cacheBeans>
    <cacheBean>
      <beanName>loadCacheService</beanName>
      <cacheMethods>
        <methodConfig>
          <methodName>getCacheName</methodName>
          <parameterTypes>
            <java-class>java.lang.String</java-class>
          </parameterTypes>
        </methodConfig>
        <methodConfig>
          <methodName>getCacheCount</methodName>
          <parameterTypes>
            <java-class>java.lang.Long</java-class>
          </parameterTypes>
        </methodConfig>
      </cacheMethods>
    </cacheBean>
  </cacheBeans>
  <cacheCleanBeans>
    <cacheCleanBean>
      <beanName>loadCacheService</beanName>
      <methods>
        <cacheCleanMethod>
          <methodName>cleanCacheByName</methodName>
          <parameterTypes>
            <java-class>java.lang.String</java-class>
          </parameterTypes>
          <cleanMethods>
            <methodConfig>
              <methodName>getCacheName</methodName>
            </methodConfig>
          </cleanMethods>
        </cacheCleanMethod>
        <cacheCleanMethod>
          <methodName>cleanCacheByCount</methodName>
          <parameterTypes>
            <java-class>java.lang.Long</java-class>
          </parameterTypes>
          <cleanMethods>
            <methodConfig>
              <methodName>getCacheCount</methodName>
            </methodConfig>
          </cleanMethods>
        </cacheCleanMethod>
      </methods>
    </cacheCleanBean>
  </cacheCleanBeans>
</cacheModule>