<?xml version="1.0" encoding="GBK"?>
<beans default-autowire="byName" xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
 http://www.springframework.org/schema/context 
 http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<context:component-scan base-package="com.taobao.pamirs.cache.load.testbean" />

	<bean id="cacheManager" class="com.taobao.pamirs.cache.load.impl.LocalConfigCacheManager" init-method="init">
		<property name="storeType">
			<value>tair</value>
		</property>
		<property name="tairNameSpace">
			<value>296</value>
		</property>
		<property name="storeRegion">
			<value>hj-daily</value>
		</property>
		<property name="configFilePaths">
			<list>
				<value>load/cache-config.xml</value>
			</list>
		</property>
	</bean>
	
</beans>